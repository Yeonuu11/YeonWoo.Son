#include <stdio.h>

void Fibo (int n){
	
	int f1=0, f2=1, f3;
	int i;
	
	printf("%d %d ",f1,f2);
	
	for(i=3;i<=n;i++){
		f3=f1+f2;
		printf("%d ",f3);
		f1=f2;
		f2=f3;
	}
}

int main (void){
	
	int n;
	
	printf("출력하고자 하는 피보나치 수열 개수 : ");
	scanf("%d",&n);
	
	if(n<2){
		printf("2 이상의 값을 입력하세요!\n");
		return -1;//비정상적인 종료 
	}
	
	Fibo(n);
	
	return 0; 
}
