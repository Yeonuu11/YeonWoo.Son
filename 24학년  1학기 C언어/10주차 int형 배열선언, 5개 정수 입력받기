#include <stdio.h>

int main (void){
	
	int A[5];
	int i,sum=0,max,min;
	
	printf("정수 5개 입력 :");
	
	for(i=0;i<5;i++){
		scanf("%d",&A[i]);
		sum += A[i]; //입력 받으면서 sum에 다 넣는다
	}
	
	max = A[0]; //max, min 변수에 제일 첫번째 값을 넣는다
	min = A[0];
	
	for(i=1;i<5;i++){
		
		if(max<A[i]) //A[0] 값과 A[1]을 비교해서, 더 큰거를 max에 넣는다
		max=A[i];
		
		if(min>A[i]) //똑같이 비교해서 더 작은거를 min에 넣는다
		min=A[i];
	}
	
	printf("총합 : %d\n",sum);
	printf("가장 큰 값 : %d\n",max);
	printf("가장 작은 값 : %d\n",min);
	
	return 0;
}